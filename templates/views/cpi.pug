                          span#basic-addon.input-group-text max   10 
                      br
                      .alert.alert-primary.text-center.text-uppercase.alert-cgpa-msg(role='alert')
                        b
                          span#cgpa-msg.cgpa-msg
                      button#submit.btn.btn-primary.float-right Submit
                br
                br
