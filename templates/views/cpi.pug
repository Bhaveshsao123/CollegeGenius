                        .input-group-append
                          span#basic-addon.input-group-text max 200 
                      br
                      .input-group.col-sm-8
                        input#cgpa.form-control.cgpa-input(type='text', maxlength='4', placeholder='CGPA till last Sem?', aria-label='Marks Input', aria-describedby='basic-addon')
                        .input-group-append
                          span#basic-addon.input-group-text   max 10 
                      br
                      .input-group.col-sm-8
                        input#c.form-control.cgpa-input(type='text', maxlength='2', placeholder='Credits in this Sem?', aria-label='Marks Input', aria-describedby='basic-addon')
                        .input-group-append
                          span#basic-addon.input-group-text max   32 
                      br
                      .input-group.col-sm-8
                        input#gpa.form-control.cgpa-input(type='text', maxlength='4', placeholder='GPA this semester?', aria-label='Marks Input', aria-describedby='basic-addon')
